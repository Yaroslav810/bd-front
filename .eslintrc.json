{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript"
  ],
  "overrides": [
    {
      "files": [
        "*.tsx",
        "*.ts"
      ],
      "rules": {
        "@typescript-eslint/explicit-function-return-type": [
          "off"
        ],
        "react/react-in-jsx-scope": [
          "off"
        ]
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "tsconfig.json"
    ]
  },
  "plugins": [
    "react"
  ],
  "rules": {
  }
}
